{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
	"extends": ["eslint:recommended", "google"],
  "rules": {
		"indent": [2, "tab"],
		"no-tabs": 0,
		"no-mixed-spaces-and-tabs": 0,
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "require-jsdoc": "off"
  }
}